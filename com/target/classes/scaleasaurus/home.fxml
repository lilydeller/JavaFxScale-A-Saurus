<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="401.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HomeController">

    <children>

    
        <Label layoutX="230.0" layoutY="50.0" text="Welcome Home!">
            <font>
                <Font size="24.0" />
            </font>
        </Label>

      
        <Button fx:id="btn_profile" layoutX="220.0" layoutY="130.0" onAction="#onProfileClicked" text="Profile" />

     
        <Button fx:id="btn_logout" layoutX="220.0" layoutY="180.0" onAction="#onLogoutClicked" text="Logout" />

    </children>
</AnchorPane>
