<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/19"
            fx:controller="controllers.HomeController"
            prefWidth="600.0" prefHeight="400.0">

    <children>

    
        <Label layoutX="230.0" layoutY="50.0" text="Welcome Home!">
            <font>
                <Font size="24.0" />
            </font>
        </Label>

      
        <Button fx:id="btn_profile"
                layoutX="220.0" layoutY="130.0"
                text="Profile"
                onAction="#onProfileClicked" />

     
        <Button fx:id="btn_logout"
                layoutX="220.0" layoutY="180.0"
                text="Logout"
                onAction="#onLogoutClicked" />

    </children>
</AnchorPane>
