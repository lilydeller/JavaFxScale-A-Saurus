<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>


<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.SongResultsController"
      prefWidth="375" prefHeight="667"
      spacing="0" styleClass="piano-root">

 
    <ScrollPane fx:id="scrollPane" fitToWidth="true" prefHeight="600">
        <content>
            <VBox fx:id="resultsContainer" spacing="10">
                <padding>
                    <Insets top="20" right="20" bottom="20" left="20"/>
                </padding>
            </VBox>
        </content>
    </ScrollPane>


    <fx:include source="navbar.fxml" />
</VBox>