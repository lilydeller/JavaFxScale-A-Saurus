<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.SongResultsController"
            prefWidth="375" prefHeight="667"
            style="-fx-background-color: #fdf5ec;"> 

    <VBox fx:id="mainVBox" spacing="0" AnchorPane.topAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="50.0">
        <ScrollPane fx:id="scrollPane" fitToWidth="true" prefHeight="600">
            <content>
                <VBox fx:id="resultsContainer" spacing="10">
                    <padding>
                        <Insets top="20" right="20" bottom="20" left="20"/>
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
    </VBox>

    <fx:include source="navbar.fxml" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" />
    
</AnchorPane>