<?xml version="1.0" encoding="UTF-8"?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="620.0" prefWidth="383.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SettingsController">
    <children>
        <Label layoutX="110.0" layoutY="40.0" text="User Settings">
            <font>
                <Font size="28.0" />
            </font>
        </Label>

        <Button fx:id="btn_editProfile" layoutX="100.0" layoutY="120.0" text="Edit Profile" onAction="#onEditProfileClicked" />
        <Button fx:id="btn_notifications" layoutX="100.0" layoutY="180.0" text="Notifications" onAction="#onNotificationsClicked" />
        <Button fx:id="btn_help" layoutX="100.0" layoutY="240.0" text="Help & Support" onAction="#onHelpClicked" />
        <Button fx:id="btn_back" layoutX="100.0" layoutY="300.0" text="Back" onAction="#onBackClicked" />
    </children>
</AnchorPane>
